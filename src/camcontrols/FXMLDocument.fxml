                            </children>
                        </AnchorPane>
                        <AnchorPane fx:id="pane4" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: yellow;" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <WebView fx:id="webView4" layoutX="155.0" layoutY="7.0" prefHeight="317.0" prefWidth="640.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
                            </children>
                        </AnchorPane>
                    </children>
                </GridPane>
                <Pane maxHeight="-Infinity" prefHeight="60.0" prefWidth="1280.0" style="-fx-background-color: purple;">
                    <children>
                        <Button fx:id="btnCamera1Bigger" layoutX="15.0" layoutY="-2.0" mnemonicParsing="false" onAction="#handleButtonCam1Bigger" text="1BIG" />
                        <Button fx:id="btnCamera2Bigger" layoutX="85.0" layoutY="-2.0" mnemonicParsing="false" onAction="#handleButtonCam2Bigger" text="2BIG" />
                        <Button fx:id="btnCamera3Bigger" layoutX="150.0" layoutY="-2.0" mnemonicParsing="false" onAction="#handleButtonCam3Bigger" text="3BIG" />
                        <Button fx:id="btnCamera4Bigger" layoutX="209.0" layoutY="-2.0" mnemonicParsing="false" onAction="#handleButtonCam4Bigger" text="4BIG" />
                        <Button fx:id="btnDefaultCameraSize" layoutX="283.0" layoutY="-2.0" mnemonicParsing="false" onAction="#handleButtonCamDefault" text="DEFAULT" />
                        <Button fx:id="btnTestStream" layoutX="422.0" layoutY="2.0" mnemonicParsing="false" onAction="#handleButtonTestStream" text="TEST STREAM" />
                    </children>
                </Pane>
            </children>
        </VBox>
    </children>
